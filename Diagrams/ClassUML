@startuml
class Class {
  - classId : String
  - className : String
  - instructorId : String
  - classType : String
  - description : String
  - daytime : Daytime[]
}

class Daytime {
  - day : String
  - time : String
  - duration : Number
}

class ClassController {
  + addClass(req, res)
  + listClasses(req, res)
}

class ClassModel

ClassController --> ClassModel : uses
ClassController --> Class : creates
@enduml
